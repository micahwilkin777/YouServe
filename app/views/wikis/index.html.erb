<p id="notice"><%= notice %></p>

<h1>Listing Wikis</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Pictureone</th>
      <th>String</th>
      <th>Picturetwo</th>
      <th>Picturethree</th>
      <th>Picturefour</th>
      <th>Picturefive</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wikis.each do |wiki| %>
      <tr>
        <td><%= wiki.title %></td>
        <td><%= wiki.description %></td>
        <td><%= wiki.pictureone %></td>
        <td><%= wiki.string %></td>
        <td><%= wiki.picturetwo %></td>
        <td><%= wiki.picturethree %></td>
        <td><%= wiki.picturefour %></td>
        <td><%= wiki.picturefive %></td>
        <td><%= link_to 'Show', wiki %></td>
        <td><%= link_to 'Edit', edit_wiki_path(wiki) %></td>
        <td><%= link_to 'Destroy', wiki, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Wiki', new_wiki_path %>
